<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Guia de Perguntas</title>
  <link rel="stylesheet" href="/css/style.css">
</head>
<body>

  <img src="/img/logo.png" alt="logo">
  
  <h1>Guia perguntas!</h1>
  <hr>
  <h3>Lista de perguntas!</h3>

  <p>Nome: <%= nome %></p>
  <p>Linguagem favorita: <%= lang %></p>
  <p>Empresa: <%= empresa %></p>
  <p>Inscritos: <%= inscritos %></p>

  <% if(msg == true){ %>
    <h3>Isso Ã© uma msg de erro!</h3>
  <%} else { %>
    <h3>Nenhum erro!</h3>
  <%}%>

  <% produtos.forEach(function(produto) { %>
    <hr>
    <h4><%= produto.nome %></h4>
    <h5><%= produto.preco %></h5>
  <% }) %> 

</body>
</html>